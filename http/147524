function postToPengyou(){
var _url = encodeURIComponent(document.location.href);
var _site = encodeURIComponent("中澳网");
var _title = encodeURIComponent("出租豪华单人房ш光_~~仅限女生~~woden~");
var _pics = "http%3A%2F%2Fwww.chnaus.com%2Fforum.php%3Fmod%3Dimage%26aid%3D84505%26size%3D400x400%26key%3Df91870308a1fcff4%26nocache%3Dyes%26type%3Dfixnone|http%3A%2F%2Fwww.chnaus.com%2Fforum.php%3Fmod%3Dimage%26aid%3D84507%26size%3D400x400%26key%3De5735e73268d609d%26nocache%3Dyes%26type%3Dfixnone";
var _from = 'discuz';
var _u = 'http://sns.qzone.qq.com/cgi-bin/qzshare/cgi_qzshare_onekey?to=pengyou&url='+_url+'&site='+_site+'&title='+_title+'&pics='+_pics+'&from='+_from;
window.open(_u);
}
</script>
	

